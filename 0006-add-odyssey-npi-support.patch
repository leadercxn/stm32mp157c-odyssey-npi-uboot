+                                { (yyval.id) = (yyvsp[-1].id); }
+    break;
+
+  case 106:
+                                { (yyval.id) = (yyvsp[-1].id); }
+    break;
+
+  case 109:
+                                        { (yyval.expr) = NULL; }
+    break;
+
+  case 110:
+                                        { (yyval.expr) = (yyvsp[0].expr); }
