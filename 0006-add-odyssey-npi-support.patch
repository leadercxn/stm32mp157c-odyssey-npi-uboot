+  case 118:
+                                                { (yyval.expr) = (yyvsp[-1].expr); }
+    break;
+
+  case 119:
+                                                { (yyval.expr) = expr_alloc_one(E_NOT, (yyvsp[0].expr)); }
+    break;
+
+  case 120:
+                                                { (yyval.expr) = expr_alloc_two(E_OR, (yyvsp[-2].expr), (yyvsp[0].expr)); }
+    break;
+
+  case 121:
